{
	"name": "heatmap",
	"label": "HeatMap: Plots z-scores of cortical thickness relative to normative data",
	"description": "Produces cortical thickness maps z-normalized to normative thickness data.",
	"version": "0.0.2_0.2.1",
	"inputs": {
		"CorticalThicknessImage": {
			"base": "file",
			"description": "Patient's cortical thickness image."
		}
	},
	"config": {
		"zthresholds": {
			"description": "space-separated list of z-score thresholds",
			"type": "number"
		},
		"maxz": {
			"description": "maximum z-score to display",
			"type": "number"
		}
	},
	"environment": {
		"FLYWHEEL": "/flywheel/v0",
		"LANG": "C.UTF-8",
		"FSLDIR": "/usr/share/fsl",
		"FSLOUTPUTTYPE": "NIFTI_GZ"
},
	"command": "/flywheel/v0/run",
	"author": "William Tackett and ",
	"maintainer": "William Tackett",
	"license": "BSD-3-Clause",
	"source": "https://github.com/willtack/heat-map",
	"url": "https://hub.docker.com/r/willtack/heatmap/tags",
	"custom": {
		"gear-builder": {
			"category": "analysis",
			"image": "willtack/heatmap:0.2.1"
		},
		"flywheel": {
			"suite": "BrainScienceCenter"
		}
	}
}
